{
    "name": "warmans/docblocker",
    "type": "library",
    "description": "Analyse inline documentation coverage in a PHP project",
    "keywords": ["code", "ci", "documentation", "metrics"],
    "homepage": "https://github.com/warmans/docblocker",
    "authors": [
        {
            "name": "Stefan Warman",
            "email": "stefan.warman@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/warmans/docblocker"
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "v2.5.0",
        "symfony/filesystem": "v2.5.0",
        "pimple/pimple": "~1.0",
        "phpdocumentor/reflection": "~1.0.5",
        "phpdocumentor/reflection-docblock": "~2.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "phploc/phploc": "2.1.*@dev"
    },
    "autoload": {
        "psr-4": { "Docblocker\\": "src/" }
    },
    "bin": [
        "docblocker"
    ]
}
